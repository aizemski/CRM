<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="426.0" prefWidth="223.0" spacing="4.0" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SignUpController">
   <children>
      <Label fx:id="nameLabel" text="Name:" />
      <TextField fx:id="name" />
      <Label fx:id="lastNameLabel" text="Last Name:" />
      <TextField fx:id="lastName" />
      <Label fx:id="loginLabel" text="Login:" />
      <TextField fx:id="login" />
      <Label fx:id="passwordLabel" text="Password:" />
      <PasswordField fx:id="password" />
      <Label fx:id="repeatPasswordLabel" text="Repat Password:" />
      <PasswordField fx:id="repeatPassowrd" />
      <Label fx:id="emailLabel" text="Email:" />
      <TextField fx:id="email" />
      <Label fx:id="phoneLabel" text="Phone" />
      <TextField fx:id="phone" />
      <HBox spacing="20.0">
         <children>
            <Button fx:id="signIn" mnemonicParsing="false" onAction="#signInAction" text="Sign In">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button fx:id="cancle" mnemonicParsing="false" onAction="#cancleAction" text="Cancle" />
            <Label fx:id="exist" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
